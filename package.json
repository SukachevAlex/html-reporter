{
  "name": "newton-reporter",
  "version": "1.1.2",
  "description": "Plugin for gemini and hermione which is intended to aggregate the results of tests running into html report",
  "main": "hermione-entry",
  "scripts": {
    "lint": "tslint -c tslint.json ./**/*.ts ./**/*.tsx -e \"node_modules\"",
    "test-unit": "NODE_ENV=test mocha test",
    "test": "run-s lint test-unit",
    "precommit": "npm run test",
    "prepublish": "npm run build",
    "start": "webpack-dev-server --config=webpack.dev.js --progress --colors --hot --inline",
    "commitmsg": "conventional-changelog-lint -e",
    "release": "standard-version",
    "uglify": "uglifyjs --compress --mangle -o ./build/hermione.min.js -- ./build/**/*.js",
    "rm-all": "cd build && rm -rf typings && cd lib/static && rm -rf components",
    "prod:webpack": "NODE_ENV=production webpack --config=webpack.prod.js",
    "prod:typescript": "tsc",
    "build": "run-s prod:typescript prod:webpack rm-all",
    "dev:webpack": "webpack --progress -d --config=webpack.dev.js --watch",
    "dev:typescript": "tsc -w",
    "watch": "run-p dev:webpack dev:typescript"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SukachevAlex/html-reporter.git"
  },
  "bugs": {
    "url": "https://github.com/SukachevAlex/html-reporter/issues"
  },
  "engines": {
    "node": ">= 6"
  },
  "keywords": [
    "gemini",
    "hermione",
    "plugin",
    "html-reporter",
    "newton-reporter"
  ],
  "homepage": "https://github.com/SukachevAlex/html-reporter#readme",
  "license": "MIT",
  "dependencies": {
    "bluebird": "3.5.3",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "express": "4.16.4",
    "fs-extra": "7.0.1",
    "gemini-configparser": "1.0.0",
    "json-stringify-safe": "5.0.1",
    "lodash": "4.17.11",
    "opener": "1.5.1",
    "resolve": "1.8.1",
    "semver": "5.6.0",
    "shelljs": "0.8.3",
    "signal-exit": "3.0.2",
    "temp": "0.8.3",
    "ts-node": "7.0.1",
    "tslib": "^1.9.3",
    "urijs": "1.19.1"
  },
  "devDependencies": {
    "typescript": "3.1.6",
    "@types/bluebird": "3.5.24",
    "@bem-react/classname": "1.1.1",
    "@types/chai": "4.1.7",
    "@types/classnames": "2.2.6",
    "@types/enzyme": "3.1.15",
    "@types/enzyme-adapter-react-16": "1.0.3",
    "@types/express": "4.16.0",
    "@types/fs-extra": "^5.0.4",
    "@types/json-stringify-safe": "^5.0.0",
    "@types/lodash": "4.14.118",
    "@types/mocha": "5.2.5",
    "@types/node": "10.12.10",
    "@types/opener": "^1.4.0",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "16.7.7",
    "@types/react-dom": "16.0.11",
    "@types/react-lazyload": "^2.3.1",
    "@types/react-redux": "6.0.10",
    "@types/react-syntax-highlighter": "0.0.8",
    "@types/react-virtualized": "^9.18.11",
    "@types/redux": "3.6.0",
    "@types/redux-logger": "3.0.6",
    "@types/redux-mock-store": "^1.0.0",
    "@types/sinon": "5.0.7",
    "@types/urijs": "1.15.38",
    "app-module-path": "2.2.0",
    "awesome-typescript-loader": "5.2.1",
    "axios": "0.18.0",
    "chai": "4.2.0",
